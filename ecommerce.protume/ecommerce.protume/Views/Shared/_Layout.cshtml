    <!-- Simple Lence JS
    ============================================ -->
    <script type="text/javascript" src="~/assets/js/jquery.simpleGallery.min.js"></script>
    <script type="text/javascript" src="~/assets/js/jquery.simpleLens.min.js"></script>
    <!-- owl.carousel JS
    ============================================ -->
    <script src="~/assets/js/owl.carousel.min.js"></script>
